---
layout: null
---
[
    {% for post in site.posts limit:10 %}
    {
        "title":        "{{ post.title }}",
        "date":         {{ post.date | date: "%d %b, %Y" | jsonify}},
        "url":          "{{ site.domain }}{{ site.baseurl }}{{ post.url }}",
        "tags":         "#{{ post.tags | join: ', #'}}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
]
